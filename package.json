{
  "name": "spaceport",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:create": "aws s3 mb s3://spaceport.codingandcommunity.org --profile codingandcommunity",
    "deploy:update": "npm run build && aws s3 sync build/ s3://spaceport-dev.codingandcommunity.org --profile codingandcommunity",
    "deploy": "yarn deploy:update"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
